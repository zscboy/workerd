---
 bazel/config/BUILD.bazel | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/bazel/config/BUILD.bazel b/bazel/config/BUILD.bazel
index c81f2782031..15d5360e2ce 100644
--- a/bazel/config/BUILD.bazel
+++ b/bazel/config/BUILD.bazel
@@ -61,7 +61,7 @@ config_setting(
 
 config_setting(
     name = "platform_cpu_arm",
-    constraint_values = ["@platforms//cpu:arm"],
+    constraint_values = ["@platforms//cpu:armv7"],
 )
 
 config_setting(
